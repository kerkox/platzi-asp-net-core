
@model List<Student>
@{
    ViewData["Title"] = "Information Student";
}

<h1>@ViewData["Title"]</h1>


<table class="table table-dark table-hover">
<thead>
    <tr>
        <th scope="col">Name</th>
        <th scope="col">Id</th>
    </tr>
</thead>
<tbody>

@foreach (var student in Model)
{
    <tr>
        <td>@student.Name</td>
        <td>@student.UniqueId</td>        
    </tr>
    
}
</tbody>
</table>


<i><i>@ViewBag.date</i></i>