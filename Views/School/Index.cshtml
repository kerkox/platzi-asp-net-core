@model School
@{
    ViewData["Title"] = "Information School";
}

<h1>School @ViewData["Title"]</h1>
<h2>Name: @Model.Name </h2>
<h3>School Type: @Model.SchoolType </h3>
<address>
    <p>Address: @Model.Address</p>
    <p>Country: @Model.Country</p>
    <p>City: @Model.City</p>
</address>
<p><strong>Foundation year : @Model.CreateYear</strong></p>

<i><i>@ViewBag.ThingDinamic</i></i>